import java.util.Scanner;
class Main{
    public static void main(String[]args){
    Scanner scan = new Scanner(System.in);
    System.out.print("the value of x: ");
    int x = scan.nextInt();
    System.out.print("the value of y: ");
    int y = scan.nextInt();
    for(int i=0;i<y;i++){
        System.out.println(x);
    }
}
}
